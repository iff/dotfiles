#!/bin/zsh
set -eu -o pipefail

prune=(
    -path ./.git
    -or -path ./bootstrap/pdocs/pdocs
    -or -path ./bootstrap/ptags/ptags
    -or -path ./bootstrap/tmux/tmux
    -or -path ./bootstrap/nvim/vimminent
    -or -path ./bootstrap/nvim/nvim
    -or -path ./vim
)

find . \( $prune \) -prune -or \( -type f -and -print \)
